{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "python",
    "julia",
    "cpp",
    "json"
  ],
  "python.linting.pylintEnabled": true,
  "python.formatting.provider": "autopep8",
  "highlight.colorNames.enable": true,
  "coc.source.file.enable": true,
  "floaterm.completion.enable": true
}
